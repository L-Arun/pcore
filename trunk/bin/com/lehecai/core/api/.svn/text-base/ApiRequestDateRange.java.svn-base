package com.lehecai.core.api;

import java.util.Date;

import com.lehecai.core.util.CoreDateUtils;

public class ApiRequestDateRange extends ApiRequestRange {

	public ApiRequestDateRange(Date begin, Date end) {
		super(begin == null ? null : CoreDateUtils.formatDate(begin), end == null ? null : CoreDateUtils.formatDate(end));
	}
}
