package com.lehecai.core.lottery.play;

/**
 * @author sunshow
 *
 */
public class GroupDefinition {
	
	public GroupDefinition(int required, int dan_max, int dan_min, int tuo_max, int tuo_min) {
		this.required = required;
		this.dan_max = dan_max;
		this.dan_min = dan_min;
		this.tuo_max = tuo_max;
		this.tuo_min = tuo_min;
	}
	
	/**
	 * 标准单式玩法的本组元素个数
	 */
	private int required;
	
	/**
	 * 允许胆码最多个数
	 */
	private int dan_max;
	
	/**
	 * 允许胆码最少个数
	 */
	private int dan_min;
	
	/**
	 * 允许拖码最多个数
	 */
	private int tuo_max;
	
	/**
	 * 允许拖码最少个数
	 */
	private int tuo_min;

	public int getRequired() {
		return required;
	}

	public void setRequired(int required) {
		this.required = required;
	}

	public int getDan_max() {
		return dan_max;
	}

	public void setDan_max(int danMax) {
		dan_max = danMax;
	}

	public int getDan_min() {
		return dan_min;
	}

	public void setDan_min(int danMin) {
		dan_min = danMin;
	}

	public int getTuo_max() {
		return tuo_max;
	}

	public void setTuo_max(int tuoMax) {
		tuo_max = tuoMax;
	}

	public int getTuo_min() {
		return tuo_min;
	}

	public void setTuo_min(int tuoMin) {
		tuo_min = tuoMin;
	}
	
}
