package com.lehecai.core.api.bean.query;


/**
 * 单一查询条件
 * @author qatang
 *
 */
public interface IQueryProp {

	/**
	 * 查询属性名
	 * @return
	 */
	public String getName();

	/**
	 * 运算符
	 * @return
	 */
